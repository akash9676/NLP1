            "text/plain": []
          },
          "metadata": {},
          "execution_count": 61
        }
      ],
      "source": [
        "import gradio as gr\n",
        "from typing import TypedDict, List, Dict\n",
        "import json\n",
        "import os\n",
        "\n",
        "# Define the Hit TypedDict\n",
        "class Hit(TypedDict):\n",
        "    cid: str\n",
        "    score: float\n",
        "    text: str\n",
        "\n",
        "# Assuming CSCBM25Retriever is already implemented and available\n",
        "# from your_module import CSCBM25Retriever\n",
        "\n",
        "# Initialize the Retriever (Ensure that the index is already built and saved)\n",
        "csc_bm25_retriever = CSCBM25Retriever(index_dir=\"output/csc_bm25_index\")\n",
        "\n",
        "def search(query: str) -> str:\n",
        "    \"\"\"\n",
        "    Given a query string, retrieve the top-k relevant documents using the BM25 retriever.\n",
        "\n",
        "    Args:\n",
        "        query (str): The user's search query.\n",
        "\n",
        "    Returns:\n",
        "        str: A JSON-formatted string representing a list of Hit dictionaries.\n",
        "    \"\"\"\n",
        "    # Retrieve the top 10 documents based on the query\n",
        "    topk_results = csc_bm25_retriever.retrieve(query, topk=10)\n",
        "\n",
        "    hits = []\n",
        "    for cid, score in topk_results.items():\n",
        "        # Retrieve the document ID from the collection ID\n",
        "        docid = csc_bm25_retriever.index.cid2docid.get(cid)\n",
        "        if docid is not None and csc_bm25_retriever.index.doc_texts is not None:\n",
        "            # Retrieve the document text\n",
        "            text = csc_bm25_retriever.index.doc_texts[docid]\n",
        "            # Append the hit to the list\n",
        "            hits.append({'cid': cid, 'score': score, 'text': text})\n",
        "\n",
        "    # Convert the list of hits to a JSON-formatted string\n",
        "    return json.dumps(hits)\n",
        "\n",
        "# Define the Gradio interface\n",
        "demo: Optional[gr.Interface] = gr.Interface(\n",
        "    fn=search,\n",
        "    inputs=gr.Textbox(\n",
        "        lines=2,\n",
        "        placeholder=\"Enter your query here...\",\n",
        "        label=\"Search Query\"\n",
        "    ),\n",
        "    outputs=gr.Textbox(label=\"Top Results\"),  # Changed from gr.JSON to gr.Textbox\n",
        "    title=\"BM25 Search Engine Demo\",\n",
        "    description=\"Enter a query to retrieve relevant documents based on BM25 ranking.\"\n",
        ")\n",
        "\n",
        "# Launch the Gradio app\n",
        "demo.launch()\n"
      ]
    },
